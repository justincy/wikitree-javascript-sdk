  "bugs": {
    "url": "https://github.com/justincy/wikitree-javascript-sdk/issues"
  },
  "homepage": "https://github.com/justincy/wikitree-javascript-sdk",
  "devDependencies": {
    "browserify": "10.0.0",
    "chai": "2.3.0",
    "coveralls": "2.11.2",
    "istanbul": "0.3.13",
    "mocha": "2.2.4",
    "q": "1.3.0"
  }
